<?xml version="1.0" encoding="UTF-8"?>
<dxl xmlns="urn:ietf:params:xml:ns:dxl0.1"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="urn:ietf:params:xml:ns:dxl0.1 dxl.xsd"
  created="<MTEntries lastn="1"><$MTEntryDate format="%Y-%m-%dT%H:%M:%S"$><$MTBlogTimezone$></MTEntries>"
  description="List of banned IPs from MovableType generated via MT-BannedList"
  dxlUri="<$MTBlogURL$>banned_list_dxl.xml">

<!--
# This template generates an DxL feed of your banned IPs.
#
# Generated by MT-banned-list plugin:
# https://github.com/impossibledreams/mt-banned-list
# 
# For details on the DxL format, see the attached Internet draft:
# draft-newton-shafranovich-distributed-blacklists-00.txt
#
-->

<MTBannedList>
  <item>
    <traceData application="blog.trackback">
      <ip4><$MTBannedListValue$></ip4>  
    </traceData>
    <sourceDxlUri><$MTBlogURL$>banned_list_dxl.xml</sourceDxlUri>
    <description>Autogenerated from MovableType Banned List via MT-BannedList plugin</description>
    <method>direct</method>
    <hops>0</hops>
    <weight>-1.0</weight>
    <created><MTEntries lastn="1"><$MTEntryDate format="%Y-%m-%dT%H:%M:%S"$><$MTBlogTimezone$></MTEntries></created>
    <lastUpdated><MTEntries lastn="1"><$MTEntryDate format="%Y-%m-%dT%H:%M:%S"$><$MTBlogTimezone$></MTEntries></lastUpdated>
  </item>
</MTBannedList>

</dxl>